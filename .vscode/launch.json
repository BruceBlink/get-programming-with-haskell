{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Haskell Debugger",
      "type": "ghc",
      "request": "launch",
      "stopOnEntry": true,
      "workspace": "${workspaceFolder}",
      "startup": "${workspaceFolder}/Main.hs",
      "ghciPrompt": "Prelude>",
      "ghciCmd": "ghci",
      "ghciEnv": {},
      "logFile": "${workspaceFolder}/debug.log",
      "logLevel": "info"
    }
  ]
}
